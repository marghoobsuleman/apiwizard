{
    "name": "marghoobsuleman/apiwizard",
    "description": "Generate Laravel models, relations, and APIs from an interactive command line",
    "keywords": ["laravel", "api", "generator", "model", "relations", "artisan"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marghoob Suleman",
            "email": "marghoobsuleman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "MarghoobSuleman\\APIWizard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarghoobSuleman\\APIWizard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarghoobSuleman\\APIWizard\\APIWizardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
